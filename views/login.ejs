            <form action="/auth" method="post">
                <input type="text" name="username" id="username"><br>
                <input type="password" name="password" id="password"><br>
                <button>send</button>
            </form>
            <div><%=err%></div>
        </main>
        <br><br>
            <div><a href="/signin">SIGN IN</a></div>
            <div><a href="/update">UPDATE</a></div>
        <footer></footer>
    </div>
